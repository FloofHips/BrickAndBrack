{
"multipart": [

    {
      "when": {"axis": "y"
      },
      "apply": {
        "model": "brickandbrack:block/poles/large_chains_link" } },
    {
      "when": {"axis": "x"
      },
      "apply": {
        "model": "brickandbrack:block/poles/large_chains_link", "x": 90, "y": 90 } },
    {
      "when": {"axis": "z"
      },
      "apply": {
        "model": "brickandbrack:block/poles/large_chains_link", "x": 90 } },

    { "when": { "north": "true" },
	"apply": { "model": "brickandbrack:block/poles/large_chains_half", "x": -90 } }, 
    { "when": { "east": "true" },
	"apply": { "model": "brickandbrack:block/poles/large_chains_half", "x": -90, "y": 90 } },
    { "when": { "south": "true" },
	"apply": { "model": "brickandbrack:block/poles/large_chains_half", "x": 90 } },
    { "when": { "west": "true" },
	"apply": { "model": "brickandbrack:block/poles/large_chains_half", "x": 90, "y": 90 } },
    { "when": { "down": "true" },
	"apply": { "model": "brickandbrack:block/poles/large_chains_half" } },
    { "when": { "up": "true" },
	"apply": { "model": "brickandbrack:block/poles/large_chains_half", "x": 180  } }
    ]
}